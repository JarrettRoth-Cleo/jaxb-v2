                          <xs:element type="xs:string" name="Supplier_DistributorDEANumber"/>
                          <xs:element type="xs:string" name="Supplier_DistributorAddress1"/>
                          <xs:element type="xs:string" name="Supplier_DistributorAddress2"/>
                          <xs:element type="xs:string" name="Supplier_DistributorCity"/>
                          <xs:element type="xs:string" name="Supplier_DistributorState"/>
                          <xs:element type="xs:string" name="Supplier_DistributorZip"/>
                          <xs:element type="xs:string" name="Supplier_DistributorDeliveryREFDate"/>
                          <xs:element type="xs:string" name="Supplier_DistributorREFNumber"/>
                          <xs:element type="xs:string" name="DirectPurchase"/>
                        </xs:choice>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:choice>
          </xs:complexType>
        </xs:element>
        <xs:element type="xs:byte" name="TotalOrderLine"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>