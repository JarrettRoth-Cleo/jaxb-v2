<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="ASN">
    <xs:complexType>
      <xs:sequence>
        <xs:element type="xs:byte" name="PurposeCode"/>
        <xs:element type="xs:int" name="ShipmentID"/>
        <xs:element type="xs:int" name="ASNDate"/>
        <xs:element type="xs:short" name="ASNTime"/>
        <xs:element type="xs:string" name="TransactionTypeCode"/>
        <xs:element name="HEADER">
          <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
              <xs:element type="xs:string" name="NumberOfPallets"/>
              <xs:element type="xs:string" name="NumberOfCases"/>
              <xs:element type="xs:string" name="NumberOfCartons"/>
              <xs:element type="xs:string" name="GrossWeight"/>
              <xs:element type="xs:string" name="GrossWeightUofM"/>
              <xs:element type="xs:string" name="SCAC"/>
              <xs:element type="xs:string" name="TransportationMethod"/>
              <xs:element type="xs:string" name="ShipmentStatusCode"/>
              <xs:element type="xs:string" name="EquipmentCode"/>
              <xs:element type="xs:string" name="EquipmentInitial"/>
              <xs:element type="xs:string" name="EquipmentNumber"/>
              <xs:element type="xs:string" name="TrackingNumber"/>
              <xs:element type="xs:string" name="BOL"/>
              <xs:element type="xs:string" name="PROInvoice"/>
              <xs:element type="xs:string" name="MasterBOL"/>
              <xs:element type="xs:string" name="SealNumber"/>
              <xs:element type="xs:int" name="ShippedDate"/>
              <xs:element type="xs:int" name="EstimatedDelivery"/>
              <xs:element type="xs:string" name="ShipToName"/>
              <xs:element type="xs:string" name="ShipToGLN"/>
              <xs:element type="xs:string" name="ShipToDEANumber"/>
              <xs:element type="xs:string" name="ShipToCode"/>
              <xs:element type="xs:string" name="ShipToAddress"/>
              <xs:element type="xs:string" name="ShipToCity"/>
              <xs:element type="xs:string" name="ShipToState"/>
              <xs:element type="xs:short" name="ShipToZip"/>
              <xs:element type="xs:string" name="ShipToStateLicenseID"/>
              <xs:element type="xs:string" name="ShipToNAB_VAWD"/>
              <xs:element type="xs:string" name="BillToName"/>
              <xs:element type="xs:string" name="BillToGLN"/>
              <xs:element type="xs:string" name="BillToDEANumber"/>
              <xs:element type="xs:string" name="BillToCode"/>
              <xs:element type="xs:string" name="BillToAddress"/>
              <xs:element type="xs:string" name="BillToCity"/>
              <xs:element type="xs:string" name="BillToState"/>
              <xs:element type="xs:short" name="BillToZip"/>
              <xs:element type="xs:string" name="BillToStateLicenseID"/>
              <xs:element type="xs:string" name="BillToNAB_VAWD"/>
              <xs:element type="xs:string" name="ShipFromName"/>
              <xs:element type="xs:string" name="ShipFromGLN"/>
              <xs:element type="xs:string" name="ShipFromDEANumber"/>
              <xs:element type="xs:string" name="ShipFromAddress"/>
              <xs:element type="xs:string" name="ShipFromCity"/>
              <xs:element type="xs:string" name="ShipFromState"/>
              <xs:element type="xs:string" name="ShipFromZip"/>
              <xs:element type="xs:string" name="ShipFromStateLicenseID"/>
              <xs:element type="xs:string" name="ShipFromNAB_VAWD"/>
              <xs:element type="xs:string" name="SellerName"/>
              <xs:element type="xs:string" name="SellerGLN"/>
              <xs:element type="xs:string" name="SellerDEANumber"/>
              <xs:element type="xs:string" name="SellerCode"/>
              <xs:element type="xs:string" name="SellerAddress"/>
              <xs:element type="xs:string" name="SellerCity"/>
              <xs:element type="xs:string" name="SellerState"/>
              <xs:element type="xs:string" name="SellerZip"/>
