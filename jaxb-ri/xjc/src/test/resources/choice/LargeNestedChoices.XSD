                          <xs:element type="xs:string" name="ManufacturerAddress2"/>
                          <xs:element type="xs:string" name="ManufacturerCity"/>
                          <xs:element type="xs:string" name="ManufacturerState"/>
                          <xs:element type="xs:int" name="ManufacturerZip"/>
                          <xs:element type="xs:string" name="ManufacturerDeliveryREFDate"/>
                          <xs:element type="xs:string" name="ManufacturerREFNumber"/>
                          <xs:element type="xs:string" name="DistributorName"/>
                          <xs:element type="xs:string" name="DistributorDEANumber"/>
                          <xs:element type="xs:string" name="DistributorAddress1"/>
                          <xs:element type="xs:string" name="DistributorAddress2"/>
                          <xs:element type="xs:string" name="DistributorCity"/>
