              <xs:element type="xs:string" name="SellerInvoiceDate"/>
              <xs:element type="xs:string" name="VendorOrder"/>
              <xs:element type="xs:string" name="VendorOrderDate"/>
              <xs:element name="DETAILS">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="PALLET" maxOccurs="unbounded" minOccurs="0">
                      <xs:complexType>
                        <xs:choice maxOccurs="unbounded" minOccurs="0">
                          <xs:element type="xs:string" name="TareSSCC"/>
                          <xs:element type="xs:string" name="PalletNumber"/>
